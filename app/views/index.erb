<div class="container">
  <!-- HEADER -->
  <!-- NAV  -->
  <div class="post_list">
    <h2>All Posts:</h2>
    <ul>
      <%@posts.each do |post| %>
        <li><a href="/posts/<%=post.id %>/show"><%= post.title %></a>
          <span><%= post.created_at.to_date %></span></li>
      <% end %>
  </div>
</div>

